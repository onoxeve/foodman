<div id="food-<%= food.id %>">
  <div class="food">
    <div class="col-xs-12">
      <div class="panel panel-default">
        <div class="panel-body" >
          <%= image_tag food.image.thumb.url, class: "img-responsive" %>
        </div>
        <div class="panel-footer">
          <div class="form-buttons">
            <%# if user_signed_in? %>
              <%= render 'likes/likes', food: food, like_counts: food.like_users.count %>
            <%# end %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>