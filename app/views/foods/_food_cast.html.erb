<div id="food-<%= food.id %>">
  <div class="photo">
    <div class="col-xs-12">
      <div class="panel panel-default">
        <div class="panel-body" >
          <%= image_tag food.image.to_s, class: "img-responsive" %>
        </div>
        <div class="panel-footer">
          <div class="form-buttons">
            <div id="like_button-<%= food.id %>">
              <%= render 'likes/like', food: food %>
            </div>
            <% if current_user == food.user %>
              <%= button_to food, method: :delete, data: { confirm: "Are you sure?" }, class: 'btn btn-default btn-sm' do %>
                <div class="glyphicon glyphicon-trash" aria-hidden="true"></div>
            <% end %>
            <% end %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<%= debug food %>